yaml file:///etc/ros/rosdep/sources.list.d/base.yaml
yaml file:///etc/ros/rosdep/sources.list.d/python.yaml
yaml file:///etc/ros/rosdep/sources.list.d/ruby.yaml
gbpdistro file:///etc/ros/rosdep/sources.list.d/fuerte.yaml fuerte
