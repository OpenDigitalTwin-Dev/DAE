# Check whether --with-module was given.
if test ${with_module+y}
then :
  withval=$with_module;
        ol_arg=
        for ol_val in $withval ; do
                ol_arg="$ol_arg module-$ol_val"
        done
        if test "${ol_arg:-set}" = set ; then
                as_fn_error $? "bad value $withval for --with-module" "$LINENO" 5
        fi
        MODULES_DIRS=$ol_arg

else $as_nop
  MODULES_DIRS=
fi

