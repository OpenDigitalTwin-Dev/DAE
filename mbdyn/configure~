               as_fn_error $? "Could not locate trilinos" "$LINENO" 5
           fi
           { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
        fi
        ac_ext=c
