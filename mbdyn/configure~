           if test "$ol_with_mfront" = "yes" ; then
               as_fn_error $? "Could not locate Mfront" "$LINENO" 5
           fi
           { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
        fi
        ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
