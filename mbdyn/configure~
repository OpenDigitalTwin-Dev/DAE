ac_compiler_gnu=$ac_cv_c_compiler_gnu

        FFLAGS="$FFLAGS"
fi


build_manuals=no

if test "$ol_enable_manuals" != no ; then
   # Extract the first word of "latex", so it can be a program name with args.
set dummy latex; ac_word=$2
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
printf %s "checking for $ac_word... " >&6; }
if test ${ac_cv_prog_LATEX+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  if test -n "$LATEX"; then
  ac_cv_prog_LATEX="$LATEX" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir$ac_word$ac_exec_ext"; then
    ac_cv_prog_LATEX="latex"
    printf "%s\n" "$as_me:${as_lineno-$LINENO}: found $as_dir$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
LATEX=$ac_cv_prog_LATEX
if test -n "$LATEX"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $LATEX" >&5
printf "%s\n" "$LATEX" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
fi


   # Extract the first word of "dvips", so it can be a program name with args.
set dummy dvips; ac_word=$2
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
printf %s "checking for $ac_word... " >&6; }
if test ${ac_cv_prog_DVIPS+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  if test -n "$DVIPS"; then
  ac_cv_prog_DVIPS="$DVIPS" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir$ac_word$ac_exec_ext"; then
    ac_cv_prog_DVIPS="dvips"
    printf "%s\n" "$as_me:${as_lineno-$LINENO}: found $as_dir$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
DVIPS=$ac_cv_prog_DVIPS
if test -n "$DVIPS"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $DVIPS" >&5
printf "%s\n" "$DVIPS" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
fi


   # Extract the first word of "ps2pdf", so it can be a program name with args.
set dummy ps2pdf; ac_word=$2
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
printf %s "checking for $ac_word... " >&6; }
if test ${ac_cv_prog_PS2PDF+y}
then :
  printf %s "(cached) " >&6
else $as_nop
  if test -n "$PS2PDF"; then
  ac_cv_prog_PS2PDF="$PS2PDF" # Let the user override the test.
else
as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
    for ac_exec_ext in '' $ac_executable_extensions; do
  if as_fn_executable_p "$as_dir$ac_word$ac_exec_ext"; then
    ac_cv_prog_PS2PDF="ps2pdf"
    printf "%s\n" "$as_me:${as_lineno-$LINENO}: found $as_dir$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

fi
fi
PS2PDF=$ac_cv_prog_PS2PDF
if test -n "$PS2PDF"; then
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $PS2PDF" >&5
printf "%s\n" "$PS2PDF" >&6; }
else
  { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
fi



   if test -z "${LATEX}"; then
      if test "$ol_enable_manuals" = yes; then
         as_fn_error 1 "latex not found" "$LINENO" 5
      fi
   else
      build_manuals=yes
   fi

   if test "$build_manuals" = yes && test -z "${DVIPS}"; then
      if test "$ol_enable_manuals" = yes; then
         as_fn_error 1 "dvips not found" "$LINENO" 5
      fi
      build_manuals=no
   fi

   if test "$build_manuals" = yes && test -z "${PS2PDF}"; then
      if test "$ol_enable_manuals" = yes; then
         as_fn_error 1 "ps2pdf not found" "$LINENO" 5
      fi
      build_manuals=no
   fi

   if test "$build_manuals" = yes; then
echo '\documentclass{report}
\usepackage{IEEEtrantools}
\usepackage{aecompl}
\usepackage{ae}
\usepackage{amsfonts}
\usepackage{amsmath}
\usepackage{amssymb}
\usepackage{bm}
\usepackage{cancel}
\usepackage{color}
\usepackage{comment}
\usepackage{enumitem}
\usepackage{fancyvrb}
\usepackage{fontenc}
\usepackage{framed}
\usepackage{graphicx}
\usepackage{html}
\usepackage{hyperref}
\usepackage{moreverb}
\usepackage{pdfpages}
\usepackage{psfrag}
\usepackage{subcaption}
\usepackage{xcolor}
\begin{document}
MBDyn manuals Latex test.
\end{document}' > conftest.tex
   fi

   if test "$build_manuals" = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for latex packages" >&5
printf %s "checking for latex packages... " >&6; }
      if ! ${LATEX} -halt-on-error conftest.tex >& /dev/null; then
         if test "$ol_enable_manuals" = yes; then
            as_fn_error 1 "${LATEX} failed" "$LINENO" 5
         fi
         { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
         build_manuals=no
      else
         { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
      fi
   fi

   if test "$build_manuals" = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking if dvips works" >&5
printf %s "checking if dvips works... " >&6; }
      if ! ${DVIPS} -o conftest.ps conftest.dvi >& /dev/null ; then
         if test "$ol_enable_manuals" = yes; then
            as_fn_error 1 "${DVIPS} failed" "$LINENO" 5
         fi
         { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
         build_manuals=no
      else
         { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
      fi
   fi

   if test "$build_manuals" = yes; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking if ps2pdf works" >&5
printf %s "checking if ps2pdf works... " >&6; }
      if ! ${PS2PDF} conftest.ps >& /dev/null; then
         if test "$ol_enable_manuals" = yes; then
            as_fn_error 1 "${PS2PDF} failed" "$LINENO" 5
         fi
         { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
         build_manuals=no
      else
         { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
printf "%s\n" "yes" >&6; }
      fi
   fi
fi

 if test "$build_manuals" = yes; then
  BUILD_MANUALS_TRUE=
  BUILD_MANUALS_FALSE='#'
else
  BUILD_MANUALS_TRUE='#'
  BUILD_MANUALS_FALSE=
