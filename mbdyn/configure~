  	ol_with_superlu="no"
fi
			# end --with-superlu

# OpenLDAP --with-wsmp

# Check whether --with-wsmp was given.
if test ${with_wsmp+y}
then :
  withval=$with_wsmp;
	ol_arg=invalid
	for ol_val in auto yes no ; do
		if test "$withval" = "$ol_val" ; then
			ol_arg="$ol_val"
		fi
	done
	if test "$ol_arg" = "invalid" ; then
		as_fn_error $? "bad value $withval for --with-wsmp" "$LINENO" 5
	fi
	ol_with_wsmp="$ol_arg"

else $as_nop
  	ol_with_wsmp="no"
fi
			# end --with-wsmp

